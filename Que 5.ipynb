   "source": [
    "import numpy as np\n",
    "x= np.arange(9).reshape(3,3)\n",
    "y=x*2\n",
    "print(\"Array 1 :\\n\",x)\n",
    "print(\"Array 2 :\\n\",y)\n",
    "n1= np.hstack((x,y))\n",
    "n2= np.vstack((x,y))\n",
    "print()\n",
    "print(\"Horizontal stack: \\n\",n1)\n",
    "print()\n",
    "print(\"Vertical stack: \\n\" ,n2);"
   ]
  }
